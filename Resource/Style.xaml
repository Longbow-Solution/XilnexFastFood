            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="PaymentButton" TargetType="{x:Type Button}">
        <Setter Property="FontFamily" Value="{DynamicResource FontStyle}"/>
        <Setter Property="FontWeight" Value="Bold"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid>
                        <Border x:Name="Border" Height="{TemplateBinding Height}" Background="White" CornerRadius="0">
                            <Border.Effect>
                                <DropShadowEffect ShadowDepth="0" BlurRadius="10" Color="#D4D4D4" />
                            </Border.Effect>
                            <ContentPresenter HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Width="Auto" />
                        </Border>
                        <!--<Border Name="BorderDIsaled1" Visibility="Collapsed" Background="{StaticResource CORE_BLACK}" />-->
                        <Grid Name="GDIsaled" Visibility="Collapsed">
                            <Border Background="{StaticResource CORE_WHITE}" Opacity="0.8" />
                            <Polygon Points="0,0 150,0, 0,150" Stroke="Black" Fill="Black" />
                            <TextBlock Style="{StaticResource H5}" Text="{Binding Label_Disabled, UpdateSourceTrigger=PropertyChanged}" Foreground="{StaticResource CORE_WHITE}" HorizontalAlignment="Left" VerticalAlignment="Top" >
                                <TextBlock.LayoutTransform>
                                    <RotateTransform Angle="-45" />
                                </TextBlock.LayoutTransform>
                            </TextBlock>
                        </Grid>

                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="false">

                            <Setter Property="Visibility" Value="Visible" TargetName="GDIsaled"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <!-- Lawrence Style-->
    <Style x:Key="CustomizeBoxButton" TargetType="{x:Type Button}">
        <Setter Property="FontFamily"  Value="/LFFSSK;component/Resource/Fonts/#DINOT-BlackItalic"/>
        <Setter Property="FontSize" Value="20"/>
        <Setter Property="FontWeight" Value="Bold"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Border x:Name="Border" Height="{TemplateBinding Height}" Background="White" BorderThickness="2" BorderBrush="{StaticResource CORE_BLACK}">
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" Width="Auto" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter Property="Background" Value="{StaticResource SUPPORT_GRAY}" TargetName="Border"/>
                            <Setter Property="Foreground" Value="White"/>
                            <Setter Property="BorderThickness" Value="0" TargetName="Border"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="true">
                            <Setter Property="Background" Value="Green" TargetName="Border"/>
                            <Setter Property="Foreground" Value="White"/>
                            <Setter Property="BorderThickness" Value="0" TargetName="Border"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!-- end Lawrence Style-->

