    <Style x:Key="resNavButton" TargetType="{x:Type Button}">
        <Setter Property="Margin" Value="3"></Setter>
        <Setter Property="Padding" Value="3"></Setter>
        <Setter Property="Background" Value="Transparent"></Setter>
        <Setter Property="FontSize" Value="16"></Setter>
        <Setter Property="OverridesDefaultStyle" Value="True"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Border Name="border" 
                            BorderThickness="1"
                            Padding="10,5,10,5"
                            BorderBrush="#666666" 
                            CornerRadius="3" 
                            Background="{TemplateBinding Background}">
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="BaseButton" TargetType="{x:Type Button}">
        <Setter Property="FontFamily" Value="{DynamicResource FontStyle}"/>
        <Setter Property="FontWeight" Value="Bold"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid>
                        <Border x:Name="Border" Height="{TemplateBinding Height}" Background="White" CornerRadius="0">
                            <Border.Effect>
                                <DropShadowEffect ShadowDepth="0" BlurRadius="10" Color="#D4D4D4" />
                            </Border.Effect>
                            <ContentPresenter HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Width="Auto" />
                        </Border>
                        <!--<Border Name="BorderDIsaled1" Visibility="Collapsed" Background="{StaticResource CORE_BLACK}" />-->
                        <Grid Name="GDIsaled" Visibility="Collapsed">
                            <Border Background="{StaticResource CORE_WHITE}" Opacity="0.8" />
                            <Polygon Points="0,0 150,0, 0,150" Stroke="Black" Fill="Black" />
                            <TextBlock Style="{StaticResource H5}" Text="{Binding Label_SoldOut, UpdateSourceTrigger=PropertyChanged}" Foreground="{StaticResource CORE_WHITE}" HorizontalAlignment="Left" VerticalAlignment="Top" >
                                <TextBlock.LayoutTransform>
                                    <RotateTransform Angle="-45" />
                                </TextBlock.LayoutTransform>
                            </TextBlock>
                        </Grid>
                        
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="false">
                            
                            <Setter Property="Visibility" Value="Visible" TargetName="GDIsaled"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="TransparentButton" TargetType="{x:Type Button}">
        <Setter Property="FontFamily" Value="{DynamicResource FontStyle}"/>
        <Setter Property="FontWeight" Value="Bold"/>
        <Setter Property="Template">
